3516. Find Closest Person

class Solution {
    public int findClosest(int x, int y, int z) {

        int dist13 = Math.abs(x-z);
        int dist23 = Math.abs(y-z);

        if(dist13 < dist23)
            return 1;
        else if(dist23 < dist13)
            return 2;
        else
            return 0;
    }
}
